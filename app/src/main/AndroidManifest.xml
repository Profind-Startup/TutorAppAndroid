<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.com.retrofind">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tutorapplogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.DetailReservationActivity"  android:label="@string/reservationAbout"/>
        <activity android:name=".activities.AddSubjectActivity"  android:label="@string/addSubject"/>
        <activity android:name=".activities.RegisterActivity" android:label="@string/textRegister"/>
        <activity android:name=".activities.ReservationActivity" android:label="@string/titleReservations"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity"  android:label="@string/titleAddSubjects"/>
    </application>

</manifest>